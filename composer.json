{
    "name": "dbtk/schema-loader",
    "description": "Load Database schema from XML",
    "keywords": ["linkorb"],
    "authors": [{
        "name": "Joost Faassen",
        "email": "j.faassen@linkorb.com"
    },{
        "name": "LinkORB Engineering Team",
        "email": "engineering@linkorb.com"
    }],
    "bin": ["bin/dbtk-schema-loader"],
    "minimum-stability": "stable",
    "require": {
        "symfony/console": "~2.4",
        "doctrine/dbal": "~2.5",
        "linkorb/database-manager": "~2.0"
    },
    "require-dev": {
        "linkorb/conventions-checker": "~1.0",
        "phpunit/phpunit": "~4.6"
    },
    "autoload": {
        "psr-4": {
            "DbTk\\SchemaLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DbTk\\SchemaLoader\\Tests\\": "tests/"
        }
    }
}
